{
  "replaceConsole": true,
  "appenders": {
    "console": {
      "type": "console",
      "layout": {
        "type": "pattern",
        "pattern": "%[[%r] [%5.5p] %c -%] %m"
      }
    },
    "file": {
      "type": "file",
      "filename": "./log/stable.log",
      "maxLogSize": 20000
    }
  },
  "categories": {
    "default": {
      "appenders": [ "file", "console" ],
      "level": "debug"
    }
  }
}
